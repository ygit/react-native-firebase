{
  "name": "pickyourtrail",
  "version": "1.1.9",
  "build": 126,
  "private": true,
  "scripts": {
    "set-staging-environment": "cp .env.staging .env",
    "set-prod-environment": "cp .env.prod .env",
    "start": "yarn set-staging-environment && node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "jest",
    "build-android": "cd android && ./gradlew assembleRelease && cd ..",
    "build-android-alpha": "yarn set-staging-environment && yarn build-android",
    "build-android-prod": "yarn set-prod-environment && yarn build-android",
    "build-ios": "fastlane ios uploadToTestflight",
    "build-ios-testflight": "yarn set-staging-environment && yarn build-ios",
    "build-ios-prod": "yarn set-prod-environment && yarn build-ios",
    "alpha-android": "fastlane android alpha",
    "run-android": "react-native run-android",
    "run-ios": "react-native run-ios",
    "pod-install": "cd ios && pod install && cd ..",
    "precommit": "pretty-quick --staged",
    "bump-version": "node bumpVersion.js",
    "debug": "react-devtools"
  },
